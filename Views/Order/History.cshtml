@model List<Order>
<h2>Order History</h2>
<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Order Date</th>
            <th>Total Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderId</td>
                <td>@order.OrderDate</td>
                <td>@order.OrderDetails.Sum(od => od.SubTotal)</td>
            </tr>
        }
    </tbody>
</table>